            #region IdlePin
            public int IdlePintoBit(int Pin1, int Pin2, int Pin3, int Pin4, int Pin5, int Pin6)
            {
                int Idle = 0;
                if (Pin1 != 1 && Pin2 != 1 && Pin3 != 1 && Pin4 != 1 && Pin5 != 1 && Pin6 != 1)
                {
                    Idle = 1;
                }
                else if (Pin1 != 4 && Pin2 != 4 && Pin3 != 4 && Pin4 != 4 && Pin5 != 4 && Pin6 != 4)
                {
                    Idle = 4;
                }
                else if (Pin1 != 7 && Pin2 != 7 && Pin3 != 7 && Pin4 != 7 && Pin5 != 7 && Pin6 != 7)
                {
                    Idle = 7;
                }
                else if (Pin1 != 10 && Pin2 != 10 && Pin3 != 10 && Pin4 != 10 && Pin5 != 10 && Pin6 != 10)
                {
                    Idle = 10;
                }
                else if (Pin1 != 13 && Pin2 != 13 && Pin3 != 13 && Pin4 != 13 && Pin5 != 13 && Pin6 != 13)
                {
                    Idle = 13;
                }
                else if (Pin1 != 16 && Pin2 != 16 && Pin3 != 16 && Pin4 != 16 && Pin5 != 16 && Pin6 != 16)
                {
                    Idle = 16;
                }
                return Idle;
            }
            #endregion
  
            #region PintoBit
            public int PintoBit(int PinNo)
            {
                int PinBitShift = 0;

                if (PinNo == 0 | PinNo == 6)
                {
                    PinBitShift = 1;
                }
                else if (PinNo == 1 | PinNo == 7)
                {
                    PinBitShift = 4;
                }
                else if (PinNo == 2 | PinNo == 8)
                {
                    PinBitShift = 7;
                }
                else if (PinNo == 3 | PinNo == 9)
                {
                    PinBitShift = 10;
                }
                else if (PinNo == 4 | PinNo == 10)
                {
                    PinBitShift = 13;
                }
                else if (PinNo == 5 | PinNo == 11)
                {
                    PinBitShift = 16;
                }
                return PinBitShift;
            }
