            #region VecArrayDecode

            public int VecArrayDecode(int Sdatapin, int vecSetNo, int[] gp0_vectors, int[] ram, out int RamData)
            {
                int count = 0;
                int i = 0;

                #region findx_location

                //Find the first X location at Sdata (bit 18), 3 == 11
                for (i = 0; i < gp0_vectors.Length; i++)
                {
                    if (Sdatapin == 0 | Sdatapin == 6 && ((gp0_vectors[i] >> 3) & 1) == 1)
                    {
                        count = i;
                        break;
                    }
                    if (Sdatapin == 1 | Sdatapin == 7 && ((gp0_vectors[i] >> 6) & 1) == 1)
                    {
                        count = i;
                        break;
                    }
                    else if (Sdatapin == 2 | Sdatapin == 8 && ((gp0_vectors[i] >> 9) & 1) == 1)
                    {
                        count = i;
                        break;
                    }
                    else if (Sdatapin == 3 | Sdatapin == 9 && ((gp0_vectors[i] >> 12) & 1) == 1)
                    {
                        count = i;
                        break;
                    }
                    else if (Sdatapin == 4 | Sdatapin == 10 && ((gp0_vectors[i] >> 15) & 1) == 1)
                    {
                        count = i;
                        break;
                    }
                    else if (Sdatapin == 5 | Sdatapin == 11 && ((gp0_vectors[i] >> 18) & 1) == 1)
                    {
                        count = i;
                        break;
                    }

                }

                #endregion

                int delay = 0; //Ram data delay 1 bit
                int j = 0;
                int[] data = new int[8];

                for (i = 0; i < 16; i = i + 2)
                {
                    if (Sdatapin == 11)
                    {
                        data[j] = (ram[count + delay + i] & 0x400000) >> 22;  //Use 0x400000 because data return at bit 22&23 since SDATA use pin10 (Ch10)
                        j++;
                    }
                    else if (Sdatapin == 10)
                    {
                        data[j] = (ram[count + delay + i] & 0x100000) >> 20;  //Use 0x100000 because data return at bit 20&21 since SDATA use pin10 (Ch10)
                        j++;
                    }
                    else if (Sdatapin == 9)
                    {
                        data[j] = (ram[count + delay + i] & 0x40000) >> 18;  //Use 0x40000 because data return at bit 18&19 since SDATA use pin9 (Ch9)
                        j++;
                    }
                    else if (Sdatapin == 8)
                    {
                        data[j] = (ram[count + delay + i] & 0x10000) >> 16;  //Use 0x10000 because data return at bit 16&17 since SDATA use pin8 (Ch8)
                        j++;
                    }
                    else if (Sdatapin == 7)
                    {
                        data[j] = (ram[count + delay + i] & 0x4000) >> 14;  //Use 0x4000 because data return at bit 14&15 since SDATA use pin7 (Ch7)
                        j++;
                    }
                    else if (Sdatapin == 6)
                    {
                        data[j] = (ram[count + delay + i] & 0x1000) >> 12;  //Use 0x1000 because data return at bit 12&13 since SDATA use pin6 (Ch6)
                        j++;
                    }
                    else if (Sdatapin == 5)
                    {
                        data[j] = (ram[count + delay + i] & 0x400) >> 10;  //Use 0x400 because data return at bit 10&11 since SDATA use pin5 (Ch5)
                        j++;
                    }
                    else if (Sdatapin == 4)
                    {
                        data[j] = (ram[count + delay + i] & 0x100) >> 8;  //Use 0x100 because data return at bit 8&9 since SDATA use pin4 (Ch4)
                        j++;
                    }
                    else if (Sdatapin == 3)
                    {
                        data[j] = (ram[count + delay + i] & 0x40) >> 6;  //Use 0x40 because data return at bit 6&7 since SDATA use pin3 (Ch3)
                        j++;
                    }
                    else if (Sdatapin == 2)
                    {
                        data[j] = (ram[count + delay + i] & 0x10) >> 4;  //Use 0x10 because data return at bit 4&5 since SDATA use pin2 (Ch2)
                        j++;
                    }
                    else if (Sdatapin == 1)
                    {
                        data[j] = (ram[count + delay + i] & 0x4) >> 2;  //Use 0x4 because data return at bit 2&3 since SDATA use pin1 (Ch1)
                        j++;
                    }
                    else if (Sdatapin == 0)
                    {
                        data[j] = (ram[count + delay + i] & 0x1) >> 0;  //Use 0x1 because data return at bit 0&1 since SDATA use pin0 (Ch0)
                        j++;
                    }

                }

                int HexData = 0;
                int databit = 7;
                for (i = 0; i < data.Length; i++)
                {
                    HexData = HexData + (data[i] << databit);
                    databit--;
                }

                RamData = HexData;
                return ret;

            }

