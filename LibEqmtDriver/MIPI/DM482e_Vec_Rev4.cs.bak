            #region VecArray_RegRead

            public int VecArray_RegRead(int MIPI_pair, int slave_add, int ExpectedData, int RegAddress, out int ReadData)
            {
                //VIO_DriveHigh(MIPI_pair);
                int ret = 0;
                int Vector_status = 0;
                int vecSetNo = 2;
                int VecfailCount = 0;
                int[] ram = new int[512];
                int size = 500;
                int[] gp0_vectors = new int[size];
                int[] gp1_vectors = new int[size];
                int SclkPin = 0;
                int SdataPin = 0;
                int SvioPin = 0;
                MipiPairToPin(MIPI_pair, out SclkPin, out SdataPin, out SvioPin);

                //if ((RegAddress & 0xE0) > 1)
                //{
                        ExtReg_Read(SclkPin, SdataPin, SvioPin, slave_add, RegAddress, ExpectedData, size, out gp0_vectors, vecSetNo);
                //}
                ////else
                //{
                //        Reg_Read0(SclkPin, SdataPin, SvioPin, slave_add, RegAddress, ExpectedData, size, out gp0_vectors, vecSetNo);
                //}
                double delay = 1 / 52e6; //change to 0 from 1
                ret += myDM.ConfigureInputChannelDelay(mipiPair[MIPI_pair].SDATA, delay);
                ret += myDM.AcquireVecEngineStatus(moduleAlias, out Vector_status);
                if (Vector_status == 0)
                {
                    ret += myDM.RunVector(moduleAlias, vecSetNo);
                }
                Thread.Sleep(1);
                while (true)
                {
                    ret += myDM.AcquireVecEngineStatus(moduleAlias, out Vector_status);

                    if (Vector_status == 0)
                    {
                        ret += myDM.AcquireVectorFailCount(moduleAlias, out VecfailCount);
                        ret += myDM.ReadHistoryRam(moduleAlias, 100, 0, vecSetNo, ram);
                        break;
                    }
                }
                if (ret != 0)
                {
                    MessageBox.Show("Ret Error = " + ret.ToString("X"), "DM482e Return Error");
                }

                VecArrayDecode(SdataPin, vecSetNo, gp0_vectors, ram, out ReadData); //Retrive Ram Data

                return ReadData;
            }
