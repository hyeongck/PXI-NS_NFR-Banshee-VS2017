        #region control mipi
        public void VIO_DriveHigh(int mipi_pair)
        {
            int ret = 0;
            int option = 0;
            int vecsetno = 2;
            int timingset = 0;
            int size = 300;
            int LL = 0x0; int LH = 0x1; int CL = 0x2; int CH = 0x3; int triX = 0x4;
            int SclkPin = 0;
            int SdataPin = 0;
            int SvioPin = 0;
            int Idle1 = 0, Idle2 = 0, Idle3 = 0, Idle4 = 0, Idle5 = 0, Idle6 = 0, Idle7 = 0, Idle8 = 0, Idle9 = 0, Idle10 = 0, Idle11 = 0;
            MipiPairToPin(mipi_pair, out SclkPin, out SdataPin, out SvioPin);
            int Svio = PintoBit(SvioPin); //convert channel# to bit#
            int ArrayCount = 0;
            int[] group0_vectors = new int[size];
            int[] group1_vectors = new int[size];
            int Vector_status = 0;

            if (SvioPin <= 5)
            {
                Idle1 = IdlePintoBit(Svio, Idle1, Idle2, Idle3, Idle4, Idle5);
                Idle2 = IdlePintoBit(Svio, Idle1, Idle2, Idle3, Idle4, Idle5);
                Idle3 = IdlePintoBit(Svio, Idle1, Idle2, Idle3, Idle4, Idle5);
                Idle4 = IdlePintoBit(Svio, Idle1, Idle2, Idle3, Idle4, Idle5);
                Idle5 = IdlePintoBit(Svio, Idle1, Idle2, Idle3, Idle4, Idle5);
                Idle6 = IdlePintoBit(Idle6, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle7 = IdlePintoBit(Idle6, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle8 = IdlePintoBit(Idle6, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle9 = IdlePintoBit(Idle6, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle10 = IdlePintoBit(Idle6, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle11 = IdlePintoBit(Idle6, Idle7, Idle8, Idle9, Idle10, Idle11);
                group0_vectors[ArrayCount] = 0x0 | LH << Svio | triX << Idle1 | triX << Idle2 | triX << Idle3 | triX << Idle4 | triX << Idle5;
                group1_vectors[ArrayCount] = 0x0 | triX << Idle6 | triX << Idle7 | triX << Idle8 | triX << Idle9 | triX << Idle10 | triX << Idle11;
                ArrayCount++;
            }
            else
            {
                Idle1 = IdlePintoBit(Idle1, Idle2, Idle3, Idle4, Idle5, Idle6);
                Idle2 = IdlePintoBit(Idle1, Idle2, Idle3, Idle4, Idle5, Idle6);
                Idle3 = IdlePintoBit(Idle1, Idle2, Idle3, Idle4, Idle5, Idle6);
                Idle4 = IdlePintoBit(Idle1, Idle2, Idle3, Idle4, Idle5, Idle6);
                Idle5 = IdlePintoBit(Idle1, Idle2, Idle3, Idle4, Idle5, Idle6);
                Idle6 = IdlePintoBit(Svio, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle7 = IdlePintoBit(Svio, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle8 = IdlePintoBit(Svio, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle9 = IdlePintoBit(Svio, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle10 = IdlePintoBit(Svio, Idle7, Idle8, Idle9, Idle10, Idle11);
                Idle11 = IdlePintoBit(Svio, Idle7, Idle8, Idle9, Idle10, Idle11);
                group0_vectors[ArrayCount] = 0x0 | triX << Idle1 | triX << Idle2 | triX << Idle3 | triX << Idle4 | triX << Idle5 | triX << Idle6;
                group1_vectors[ArrayCount] = 0x0 | LH << Svio | triX << Idle7 | triX << Idle8 | triX << Idle9 | triX << Idle10 | triX << Idle11;
                ArrayCount++;
            }

            ret += myDM.DPINVecLoadArray(moduleAlias, option, timingset, 0, vecsetno, ArrayCount, group0_vectors, group1_vectors);

            double delay = 2 / 52e6; //(0/1/2)
            ret += myDM.ConfigureInputChannelDelay(mipiPair[mipi_pair].SDATA, delay);
            ret += myDM.AcquireVecEngineStatus(moduleAlias, out Vector_status);

            if (Vector_status == 0)
            {
                ret += myDM.RunVector(moduleAlias, vecsetno);
            }

        }
